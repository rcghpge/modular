# Qwen3VL integration tests.

load(
    "//bazel:api.bzl",
    "modular_py_test",
    "requirement",
)

modular_py_test(
    name = "test_vision_functions",
    srcs = ["test_vision_functions.py"] + glob(["utils/*.py"]),
    data = [
        "configs/qwen3-VL-30B-A3B-Instruct.json",
    ],
    target_compatible_with = select({
        "//:apple_gpu": ["@platforms//:incompatible"],
        "//conditions:default": [],
    }),  # FIXME: MOCO-2411
    deps = [
        "//SDK/lib/API/python/max/pipelines/architectures",
        requirement("torch"),
        requirement("numpy"),
    ],
)
