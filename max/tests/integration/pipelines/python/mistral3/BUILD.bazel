# Mistral3 integration tests.

load(
    "//bazel:api.bzl",
    "modular_py_test",
    "requirement",
)

package(default_visibility = ["//max/tests:__subpackages__"])

modular_py_test(
    name = "tests",
    size = "medium",
    srcs = glob(["test_*.py"]),
    deps = [
        "//max/python/max/pipelines/architectures",
        "//max/python/max/pipelines/lib",
        requirement("pytest-mock"),
    ],
)
