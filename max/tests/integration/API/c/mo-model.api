// RUN: modular-api-executor --model-inputs=zeros  --num-threads=0 --result-output-style=full %s | FileCheck %s --check-prefix=CHECK-EXECUTE
// RUN: modular-api-executor --print-metadata %s | FileCheck %s --check-prefix=CHECK-METADATA

// RUN: mt --compile %s -o=%t.mef
// RUN: modular-api-executor --model-inputs=zeros  --num-threads=0 --result-output-style=full %t.mef | FileCheck %s --check-prefix=CHECK-EXECUTE-MEF

// CHECK-EXECUTE: tensor<5xf32> [3.000000e+00, 1.000000e+00, -6.000000e+00, 2.000000e+00, 5.000000e+00]
// CHECK-EXECUTE-MEF: tensor<5xf32> [3.000000e+00, 1.000000e+00, -6.000000e+00, 2.000000e+00, 5.000000e+00]
mo.graph @add(%0: !mo.tensor<[5], f32>) -> (!mo.tensor<[5], f32>) attributes {argument_names = ["input"], result_names = ["output"]} {
  %1 = "mo.constant"() {value = #M.dense_array<3.0, 1.0, -6.0, 2.0, 5.0> : tensor<5xf32>} : () -> !mo.tensor<[5], f32>
  %2 = mo.add(%0, %1) : !mo.tensor<[5], f32>
  mo.output %2 : !mo.tensor<[5], f32>
}

// CHECK-METADATA: Model Metadata:
// CHECK-METADATA: Inputs:
// CHECK-METADATA: - name: input
// CHECK-METADATA:   spec: 5xf32
// CHECK-METADATA: Outputs:
// CHECK-METADATA: - name: output
// CHECK-METADATA:   spec: 5xf32
