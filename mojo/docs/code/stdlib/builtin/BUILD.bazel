load("//bazel:api.bzl", "mojo_binary")

package(default_visibility = ["//oss/modular/docs:__subpackages__"])

MOJO_SRCS = glob(["*.mojo"])

[
    mojo_binary(
        name = src.split(".")[0],
        srcs = [src],
        deps = [
            "@mojo//:std",
        ],
    )
    for src in MOJO_SRCS
]
